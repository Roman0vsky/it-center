				</div>
				<ul class="control-panel__list">

					<ul class="control-panel__list">

						<li class="control-panel__list-item">
							<a class="control-panel__list-link control-panel__inner-link" href=@Url.Content("/api/Admin/Teachers")>
								<div class="control-panel__list-img-wrapper">
								</div>Преподаватели
							</a>
						</li>
						</li>

					</ul>

					</a>

					<ul class="control-panel__list">
						<li class="control-panel__list-item">
							<a class="control-panel__list-link control-panel__inner-link" href=@Url.Content("/api/Admin/News")>
								<div class="control-panel__list-img-wrapper">
								</div>Новости
							</a>
						</li>

						</a>
						<ul class="control-panel__list">
							<li class="control-panel__list-item">
								<a class="control-panel__list-link control-panel__inner-link" href=@Url.Content("/api/Admin/SocialLinks")>
									<div class="control-panel__list-img-wrapper">
									</div>Ссылки
								</a>
							</li>
							<li class="control-panel__list-item">
								<a class="control-panel__list-link control-panel__inner-link" href=@Url.Content("/api/Admin/SliderImages")>
