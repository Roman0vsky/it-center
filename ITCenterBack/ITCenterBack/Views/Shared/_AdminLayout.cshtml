

<!DOCTYPE html>
<html lang="ru">

<head>
	<meta charset="UTF-8" />
	<meta http-equiv="X-UA-Compatible" content="ie=edge" />
	<title>@ViewBag.Title - Админ панель</title>
	<meta name="keywords" />
	<meta name="description" /><!-- build:css1 css/vendor.css -->
	<!-- endbuild -->
	<!-- build:css2 css/styles.css -->
	<link rel="stylesheet" href="/assets/for_new/css/main.css" /><!-- endbuild -->
